<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_hms_patient" model="ir.sequence">
            <field name="name">TCB Patient</field>
            <field name="code">hms.patient</field>
            <field name="prefix">PID</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_hms_physician" model="ir.sequence">
            <field name="name">TCB Physician</field>
            <field name="code">hms.physician</field>
            <field name="prefix">DR</field>
            <field name="padding">4</field>
        </record>
        <record id="seq_hms_appointment" model="ir.sequence">
            <field name="name">TCB Appointment</field>
            <field name="code">hms.appointment</field>
            <!-- <field name="prefix">APP</field> -->
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
