<?xml version="1.0" encoding="UTF-8"?>
<odoo>
        <record id="res_company_form_inherit" model="ir.ui.view">
            <field name="name">res.company.inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='currency_id']" position="after">
                    <field name="nabh_logo" widget="image"/>
                    <field name="pm_jay_logo" widget="image"/>
                    <field name="pain_level_diagram" widget="image"/>
                    <field name="company_timing"/>
                </xpath>
            </field>
        </record>
</odoo>